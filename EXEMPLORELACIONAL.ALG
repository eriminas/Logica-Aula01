Algoritmo "ExemploRelacional"

Var
  a, b, c: inteiro

Inicio
Escreva("Digite o primeiro número: ")
Leia(a)
Escreva("Digite o segundo número: ")
Leia(b)
Escreva("Digite o terceiro número: ")
Leia(c)

Escreval(a," =",b , a=b)
Escreval(a," >",b , a>b)
Escreval(a," >=",b , a>=b)
Escreval(c," <",b , c<b)
Escreval(c," <>",b , c<>b)

Fimalgoritmo